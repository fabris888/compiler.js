<script type="text/javascript">

var linguaggio=prompt("Vuoi inserire una Variabile?");
var variabile=linguaggio;
if(variabile==null){
window.confirm("non vuoi digitare niente?");
}
else{
alert(linguaggio);
}
var linguaggio=prompt("Vuoi inserire una costante?");
var costante=linguaggio;
if(costante==null){
window.confirm("non vuoi digitare niente?");
}
else{
alert(linguaggio);
}
</script>

